{
  "name": "rentacar-main",
  "version": "1.0.0",
  "description": "Monorepo multi-marca para alquilatucarro, alquilame y alquicarros",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "nuxt build --dotenv .env.prod",
    "build:ci": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nuxt build --dotenv .env.prod",
    "build:lite": "cross-env PRERENDER_LIMIT=true PRERENDER_CONCURRENCY=1 NODE_OPTIONS=--max-old-space-size=4096 nuxt build --dotenv .env.prod",
    "postbuild": "cp .env.prod .output/server/.env",
    "dev": "nuxt dev --dotenv .env.prod",
    "generate": "nuxt generate --dotenv .env.prod",
    "preview": "nuxt preview --dotenv .env.local",
    "postinstall": "nuxt prepare",
    "clean": "rm -rf .nuxt .output node_modules/.cache && echo cleaned",
    "firebase:preview": "sh -c 'pnpm run build && firebase hosting:channel:deploy ${1:-preview} --expires ${2:-10m}' --",
    "firebase:emulate": "pnpm run build && firebase emulators:start",
    "firebase:deploy": "pnpm run build && firebase deploy --only hosting,functions:rentacar_main_alquilatucarro",
    "scrape:google-docs": "npx tsx scripts/docs-scraper/run.ts",
    "scrape:google-docs:resume": "npx tsx scripts/docs-scraper/run.ts --resume",
    "scrape:google-docs:dry-run": "npx tsx scripts/docs-scraper/run.ts --dry-run",
    "scrape:nngroup": "npx tsx scripts/nngroup-scraper/run.ts",
    "scrape:nngroup:resume": "npx tsx scripts/nngroup-scraper/run.ts --resume",
    "scrape:nngroup:dry-run": "npx tsx scripts/nngroup-scraper/run.ts --dry-run",
    "scrape:ahrefs": "npx tsx scripts/ahrefs-scraper/run.ts",
    "scrape:ahrefs:resume": "npx tsx scripts/ahrefs-scraper/run.ts --resume",
    "scrape:ahrefs:dry-run": "npx tsx scripts/ahrefs-scraper/run.ts --dry-run",
    "scrape:backlinko": "npx tsx scripts/backlinko-scraper/run.ts",
    "scrape:seo-gaps": "npx tsx scripts/seo-gaps-scraper/download-seo-gaps.ts",
    "test:e2e": "cross-env BRAND=alquilatucarro playwright test",
    "test:e2e:ui": "cross-env BRAND=alquilatucarro playwright test --ui",
    "test:e2e:headed": "cross-env BRAND=alquilatucarro playwright test --headed",
    "test:e2e:debug": "cross-env BRAND=alquilatucarro playwright test --debug",
    "test:e2e:chromium": "cross-env BRAND=alquilatucarro playwright test --project=chromium",
    "test:e2e:chromium-mobile": "cross-env BRAND=alquilatucarro playwright test --project='Mobile Chrome'",
    "test:e2e:alquilatucarro": "cross-env BRAND=alquilatucarro playwright test",
    "test:e2e:alquicarros": "cross-env BRAND=alquicarros playwright test",
    "test:e2e:alquilame": "cross-env BRAND=alquilame playwright test",
    "test:e2e:chromium:alquilatucarro": "cross-env BRAND=alquilatucarro playwright test --project=chromium",
    "test:e2e:chromium:alquicarros": "cross-env BRAND=alquicarros playwright test --project=chromium",
    "test:e2e:chromium:alquilame": "cross-env BRAND=alquilame playwright test --project=chromium",
    "test:e2e:report": "playwright show-report e2e-results",
    "dev:alquilame": "pnpm --filter ui-alquilame dev --port 3002",
    "dev:alquicarros": "pnpm --filter ui-alquicarros dev --port 3001",
    "dev:alquilatucarro": "pnpm --filter ui-alquilatucarro dev",
    "dev:all": "pnpm --parallel --filter 'ui-*' dev",
    "build:alquilame": "pnpm --filter ui-alquilame build",
    "build:alquicarros": "pnpm --filter ui-alquicarros build",
    "build:alquilatucarro": "pnpm --filter ui-alquilatucarro build",
    "build:all": "pnpm --filter ui-alquilatucarro build && pnpm --filter ui-alquilame build && pnpm --filter ui-alquicarros build",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.80",
    "@internationalized/date": "^3.10.0",
    "@nuxt/content": "^3.10.0",
    "@nuxt/ui": "^4.2.1",
    "@nuxtjs/seo": "3.2.2",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/postcss": "^4.1.15",
    "@tailwindcss/vite": "^4.1.15",
    "@unhead/schema-org": "^2.0.19",
    "@valibot/i18n": "^1.0.0",
    "@vueuse/core": "13.9.0",
    "better-sqlite3": "^12.6.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.6.0",
    "js-confetti": "^0.13.1",
    "libphonenumber-js": "^1.12.31",
    "nuxt": "^4.1.3",
    "nuxt-llms": "^0.1.3",
    "nuxt-schema-org": "5.0.9",
    "nuxt-vitalizer": "2.0.0",
    "pinia": "^3.0.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "runtime": "link:@nuxt/image/runtime",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3",
    "ufo": "^1.6.1",
    "valibot": "^1.1.0",
    "vitest": "^4.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tel-input": "^9.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "protobufjs",
      "sharp",
      "vue-demi"
    ],
    "ignoredBuiltDependencies": [
      "sharp"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/turndown": "^5.0.5",
    "cross-env": "^10.1.0",
    "googleapis": "^170.1.0",
    "playwright": "^1.49.0",
    "tsx": "^4.19.0",
    "turndown": "^7.2.0",
    "vue-tsc": "^3.2.2"
  }
}
